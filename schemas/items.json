        "Armor",
        "SpellBlock",
        "Active",
        "LifeSteal",
        "SpellVamp",
        "Jungle",
        "OnHit",
        "Consumable",
        "Lane",
        "Stealth",
        "Vision",
        "Tenacity",
        "Trinket",
        "GoldPer",
        "ArmorPenetration",
        "AttackSpeed",
        "CooldownReduction",
        "AbilityHaste",
        "Slow",
        "MagicPenetration",
