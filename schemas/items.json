            }
          ]
        },
        "maps": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "gold": {
          "$ref": "#/definitions/Gold"
        },
        "into": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "mythic": {
          "type": "boolean"
        },
        "icon": {
          "type": "string",
          "qt-uri-protocols": [
            "https"
          ],
          "qt-uri-extensions": [
            ".png"
          ]
        },
        "placeholder": {
          "type": "string"
