        "stats": {
          "$ref": "#/definitions/Stats"
        },
        "classes": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Class"
          }
        },
        "categories": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Category"
          }
        },
        "inStore": {
          "type": "boolean"
        },
        "maxStacks": {
          "type": "integer"
        },
        "from": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        }
      },
      "required": [
        "categories",
        "classes",
        "description",
        "from",
        "gold",
        "icon",
        "iconOverlay",
